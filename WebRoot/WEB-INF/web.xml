<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>Restaurant</display-name>
	<welcome-file-list>
		<welcome-file>FrontListServlet</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>DishesServlet</servlet-name>
		<servlet-class>servlet.DishesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DishesServlet</servlet-name>
		<url-pattern>/DishesServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>FoodServlet</servlet-name>
		<servlet-class>servlet.FoodServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FoodServlet</servlet-name>
		<url-pattern>/FoodServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DinnerTableServlet</servlet-name>
		<servlet-class>servlet.DinnerTableServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DinnerTableServlet</servlet-name>
		<url-pattern>/DinnerTableServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>FrontListServlet</servlet-name>
		<servlet-class>frontservlet.FrontListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FrontListServlet</servlet-name>
		<url-pattern>/app</url-pattern>
		<url-pattern>/FrontListServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SysServlet</servlet-name>
		<servlet-class>servlet.SysServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SysServlet</servlet-name>
		<url-pattern>/sys</url-pattern>
	</servlet-mapping>
	<!-- <filter> <filter-name>RequestFilter</filter-name> <filter-class>filter.RequestFilter</filter-class> 
		</filter> <filter-mapping> <filter-name>RequestFilter</filter-name> <url-pattern>/*</url-pattern> 
		<dispatcher>REQUEST</dispatcher> </filter-mapping> -->

</web-app>